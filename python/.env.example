# AudioBookshelf Configuration
# Copy this file to .env and fill in your values

# Your AudioBookshelf server URL (must be HTTPS for production)
AUDIOBOOKSHELF_URL=https://your-audiobookshelf-server.com

# Your AudioBookshelf API token or JWT token
AUDIOBOOKSHELF_TOKEN=your_api_token_here

# Flask configuration
DEBUG=False
PORT=5000

# Optional: Default library ID
# DEFAULT_LIBRARY_ID=your_library_id
